{
  "name": "palavyr-chat-widget",
  "private": false,
  "version": "1.2.0",
  "description": "Palavyr Chat Widget - a react component that allows you to place a chat widget compatible with Palavyr in your website.",
  "repository": "git@github.com:Palavyr/palavyr-chat-widget.git",
  "bugs": {
    "url": "https://github.com/Palavyr/palavyr-chat-widget/issues/new/choose"
  },
  "license": "MIT",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "engines": {
    "node": ">=14"
  },
  "scripts": {
    "pub": "npm run build && npm run build-demo && npm run deploy && npm publish",
    "start": "node-sass demo/app.scss -o demo/ && webpack serve --config webpack.config.demo.js",
    "scss": "node-sass --watch src/components/styles.scss -o src/components/",
    "build": "rimraf dist && npm run build-scss && tsc -p prod.tsconfig.json && npm run build-copyfiles",
    "build-demo": "webpack --config ./webpack.config.demo.js",
    "build-scss": "node-sass src/components/styles.scss -o src/components/",
    "build-copyfiles": "copyfiles -u 2 src/components/styles.css dist/components/ && copyfiles -u 1 src/components/assets/* dist/",
    "deploy": "gh-pages -d build",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -p prod.tsconfig.json"
  },
  "keywords": [
    "palavyr",
    "iframe",
    "react",
    "chat",
    "widget",
    "javascript"
  ],
  "dependencies": {
    "classnames": "^2.3.1",
    "copyfiles": "^2.4.1",
    "process": "^0.11.10",
    "typescript-plugin-css-modules": "^3.4.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.14.0",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.8.3",
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-env": "^7.14.1",
    "@babel/preset-react": "^7.13.13",
    "@babel/preset-typescript": "^7.8.3",
    "@types/classnames": "^2.2.10",
    "@types/react": "^16.9.23",
    "@types/react-dom": "^16.9.5",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "autoprefixer": "^8.2.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.2",
    "babel-plugin-module-resolver": "^4.0.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-runtime": "^6.26.0",
    "clean-webpack-plugin": "^4.0.0-alpha.0",
    "copy-webpack-plugin": "^10.2.0",
    "css-loader": "^5.2.4",
    "css-minimizer-webpack-plugin": "^3.2.0",
    "dotenv-webpack": "^7.0.3",
    "eslint": "^6.8.0",
    "eslint-config-wolox": "^4.0.0",
    "eslint-loader": "^3.0.3",
    "eslint-plugin-flowtype": "^2.30.4",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-prettier": "^3.4.0",
    "file-loader": "^0.11.2",
    "gh-pages": "^3.2.3",
    "html-webpack-plugin": "^5.3.1",
    "husky": "^4.2.3",
    "mini-css-extract-plugin": "^1.5.1",
    "node-sass": "^4.14.1",
    "postcss": "^8.4.5",
    "postcss-cli": "^9.1.0",
    "postcss-loader": "^5.2.0",
    "postcss-preset-env": "^6.7.0",
    "postcss-scss": "^4.0.2",
    "prettier": "^1.19.1",
    "prettier-eslint": "^9.0.2",
    "sass-loader": "^11.1.1",
    "source-map-loader": "^2.0.1",
    "style-loader": "^2.0.0",
    "ts-loader": "^9.1.1",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.2.4",
    "url-loader": "^4.1.1",
    "webpack": "^5.36.1",
    "webpack-cli": "^4.9.1",
    "webpack-dev-server": "^3.11.2"
  },
  "peerDependencies": {
    "react": "^16.8.6 || ^17.0.0",
    "react-dom": "^16.8.6 || ^17.0.0"
  },
  "moduleFileExtensions": [
    "ts",
    "tsx",
    "js",
    "jsx",
    "json",
    "node"
  ],
  "eslintConfig": {
    "extends": [
      "react-app"
    ],
    "parser": "babel-eslint"
  },
  "files": [
    "dist/**/*"
  ]
}